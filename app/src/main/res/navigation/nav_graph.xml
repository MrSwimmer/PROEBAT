<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_host"
    app:startDestination="@id/statisticFragment">

    <fragment
        android:id="@+id/missFragment"
        android:name="com.memebattle.proebat.fragment.MissFragment"
        android:label="fragment_miss"
        tools:layout="@layout/fragment_miss" >
        <action
            android:id="@+id/action_missFragment_to_alreadyMissFragment"
            app:destination="@id/alreadyMissFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.memebattle.proebat.fragment.SettingsFragment"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings" />
    <fragment
        android:id="@+id/statisticFragment"
        android:name="com.memebattle.proebat.fragment.statistics.StatisticFragment"
        android:label="fragment_statictis"
        tools:layout="@layout/fragment_statictis" />
    <fragment
        android:id="@+id/alreadyMissFragment"
        android:name="com.memebattle.proebat.fragment.AlreadyMissFragment"
        android:label="fragment_already_miss"
        tools:layout="@layout/fragment_already_miss" />
</navigation>
